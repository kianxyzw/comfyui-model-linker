  "patterns": {
    "fp16_variants": {
      "description": "FP16/FP32 variants of the same model",
      "pattern": "(.+?)[-_]?(fp16|fp32|fp8)?\\.safetensors$",
      "note": "Models with fp16/fp32/fp8 suffix are often interchangeable"
    },
    "quantized_variants": {
      "description": "Quantized variants (Q4, Q8, GGUF)",
      "pattern": "(.+?)[-_]?(q4|q8|gguf)?\\.safetensors$"
    }
  }
